# web agent Cloud API Configuration
# Copy this file to .env and fill in your values

# Required: Your web agent Cloud API key
# Get it from: https://cloud.web-agent.com/new-api-key
web_agent_API_KEY=your_api_key_here

# Optional: Custom API base URL (for enterprise installations)
# web_agent_BASE_URL=https://api.web-agent.com/api/v1

# Optional: Default model preference
# web_agent_DEFAULT_MODEL=gemini-2.5-flash

# Optional: Cost limits
# web_agent_MAX_COST_PER_TASK=5.0

# Optional: Request timeout (seconds)
# web_agent_TIMEOUT=30

# Optional: Logging configuration  
# LOG_LEVEL=INFO
